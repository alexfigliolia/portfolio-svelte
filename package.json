{
  "name": "portfolio",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "tsc --noemit && eslint . --fix && prettier --plugin-search-dir . --check --write .",
    "format": "prettier --plugin-search-dir . --write .",
    "deploy": "yarn build && yarn gh-pages -d build -t true"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@neoconfetti/svelte": "^1.0.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.20.4",
    "@types/cookie": "^0.5.1",
    "@types/jquery": "^3.5.16",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-svelte": "^2.30.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "gh-pages": "^5.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.63.6",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "svelte-preprocess-sass": "^2.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "vitest": "^0.32.2"
  },
  "type": "module",
  "dependencies": {
    "jquery": "^3.7.0",
    "pageswitch": "https://github.com/alexfigliolia/pw.git",
    "ripples": "https://github.com/alexfigliolia/jquery.ripples.git",
    "svelte-eslint-parser": "^0.32.1"
  }
}
